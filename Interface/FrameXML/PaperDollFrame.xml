					<OnLeave>
						self.texture:SetAlpha(0.5);
						GameTooltip_Hide();
					</OnLeave>
					<OnMouseDown function="GearSetEditButton_OnMouseDown"/>
					<OnMouseUp>
						self.texture:SetPoint("TOPLEFT", 0, 0);
					</OnMouseUp>
				</Scripts>
			</DropDownToggleButton>
		</Frames>
		<Scripts>
