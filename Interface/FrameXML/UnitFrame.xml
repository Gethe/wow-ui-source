<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
UI_shared.xsd">
	<Script file="UnitFrame.lua"/>
	<Frame name="MyHealPredictionBarTemplate" virtual="true" inherits="StatusBarOverlaySegmentTemplate">
		<KeyValues>
			<KeyValue key="fillColor" value="HEALTHBAR_MY_HEAL_PREDICTION_COLOR" type="global"/>
		</KeyValues>
	</Frame>
	<Frame name="OtherHealPredictionBarTemplate" virtual="true" inherits="StatusBarOverlaySegmentTemplate">
		<KeyValues>
			<KeyValue key="fillColor" value="HEALTHBAR_OTHER_HEAL_PREDICTION_COLOR" type="global"/>
		</KeyValues>
	</Frame>
	<Frame name="TotalAbsorbBarTemplate" virtual="true" inherits="LinedStatusBarOverlaySegmentTemplate">
		<KeyValues>
			<KeyValue key="fillColor" value="HEALTHBAR_TOTAL_ABSORB_COLOR" type="global"/>
		</KeyValues>
	</Frame>
	<Frame name="HealAbsorbBarTemplate" virtual="true" inherits="ShadowedStatusBarOverlaySegmentTemplate">
		<KeyValues>
			<KeyValue key="fillColor" value="HEALTHBAR_HEAL_ABSORB_COLOR" type="global"/>
		</KeyValues>
	</Frame>

	<Texture name="OverAbsorbGlowTemplate" virtual="true" file="Interface\RaidFrame\Shield-Overshield" alphaMode="ADD">
		<Size x="16" y="0"/>
	</Texture>
	<Texture name="OverHealAbsorbGlowTemplate" virtual="true" file="Interface\RaidFrame\Absorb-Overabsorb" alphaMode="ADD">
		<Size x="16" y="0"/>
	</Texture>

	<Frame name="ManaCostPredictionBarTemplate" virtual="true" inherits="StatusBarOverlaySegmentTemplate">
		<KeyValues>
			<KeyValue key="fillColor" value="MANAPREDICTIONBLUE" type="global"/>
		</KeyValues>
	</Frame>
</Ui>
