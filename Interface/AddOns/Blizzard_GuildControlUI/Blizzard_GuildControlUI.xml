			<Layer level="OVERLAY">
				<FontString inherits="GameFontNormal" text="RANK" justifyH="RIGHT" parentKey="rankLabel">
					<Size  x="0" y="12"/>
					<Anchors>
						<Anchor point="RIGHT" relativeTo="$parentNameEditBox" relativePoint="LEFT" x="-12" y="0"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
