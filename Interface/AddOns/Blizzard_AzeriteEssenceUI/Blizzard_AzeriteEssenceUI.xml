		</Animations>
		<Scripts>
			<OnLoad>
				self.Anim.Start:SetStartDelay(self.startDelay);
				self.Anim.Rotation:SetDegrees(self.baseRotation);
				self.Anim:Play();
			</OnLoad>
		</Scripts>
