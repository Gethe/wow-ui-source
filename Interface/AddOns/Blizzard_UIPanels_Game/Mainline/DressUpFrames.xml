						<Anchor point="TOPLEFT"/>
					</Anchors>
				</Texture>
				<Texture file="Interface\AuctionFrame\AuctionHouseDressUpFrame-Bottom" texelSnappingBias="0.0" snapToPixelGrid="false">
					<Size x="256" y="256"/>
