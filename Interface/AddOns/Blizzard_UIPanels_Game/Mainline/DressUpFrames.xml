				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="2" y="4"/>
				</Anchors>
				<Scripts>
					<OnShow method="OnShow"/>
					<OnHide method="OnHide"/>
					<OnClick method="OnClick"/>
				</Scripts>
			</DropdownButton>
