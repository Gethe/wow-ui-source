<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">

<Frame name="PlayerChoiceTimeRemaining" mixin="PlayerChoiceTimeRemainingMixin" frameStrata="HIGH" parent="UIParent" toplevel="true" hidden="true">
	<Size x="300" y="30"/>
	<Anchors>
		<Anchor point="CENTER"/>
	</Anchors>
	<Layers>
		<Layer level="BACKGROUND">
			<Texture parentKey="BGShadow" atlas="ui-frame-cypherchoice-countdownshadow" useAtlasSize="true">
				<Anchors>
					<Anchor point="CENTER"/>
				</Anchors>
			</Texture>
		</Layer>
		<Layer level="OVERLAY">
			<FontString parentKey="TimerText" inherits="GameFontNormalLarge" justifyH="CENTER" maxLines="1">
				<Size x="300" y="30"/>
				<Anchors>
					<Anchor point="CENTER"/>
				</Anchors>
			</FontString>
		</Layer>
	</Layers>
</Frame>
	
</Ui>
