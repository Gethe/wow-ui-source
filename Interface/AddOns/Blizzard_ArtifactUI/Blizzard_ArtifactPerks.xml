			<OnLoad>
				self.UpdateTooltip = self:GetScript("OnEnter");
			</OnLoad>
			<OnEnter>
				self:GetParent():OnRelicSlotMouseEnter(self);
			</OnEnter>
			<OnLeave>
				self:GetParent():OnRelicSlotMouseLeave(self);
			</OnLeave>
			<OnClick>
				self:GetParent():OnRelicSlotClicked(self);
			</OnClick>
			<OnReceiveDrag>
				self:GetParent():OnRelicSlotClicked(self);
			</OnReceiveDrag>
		</Scripts>
