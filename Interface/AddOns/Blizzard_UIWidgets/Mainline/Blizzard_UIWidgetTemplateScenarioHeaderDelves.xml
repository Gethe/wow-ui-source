<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\Blizzard_SharedXML\UI.xsd">
	<Frame name="UIWidgetTemplateScenarioHeaderDelves" inherits="UIWidgetBaseScenarioHeaderTemplate" virtual="true" mixin="UIWidgetTemplateScenarioHeaderDelvesMixin">
		<Layers>
			<Layer level="ARTWORK">
				<Texture parentKey="TierFlag">
					<Anchors>
						<Anchor point="TOPRIGHT" x="-20" y="-2"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<FontString parentKey="TierText" inherits="GameFontHighlightLarge">
					<Anchors>
						<Anchor point="TOP" relativeKey="$parent.TierFlag" x="-2" y="-7"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Frame parentKey="CurrencyContainer" inherits="HorizontalLayoutFrame" frameStrata="HIGH">
				<KeyValues>
					<KeyValue key="spacing" value="10" type="number"/>
				</KeyValues>
				<Anchors>
					<Anchor point="RIGHT" relativePoint="BOTTOMRIGHT" x="-23" y="30" />
				</Anchors>
			</Frame>
			<Frame parentKey="SpellContainer" inherits="HorizontalLayoutFrame" frameStrata="HIGH">
				<KeyValues>
					<KeyValue key="spacing" value="5" type="number"/>
				</KeyValues>
				<Anchors>
					<Anchor point="LEFT" relativePoint="BOTTOMLEFT" x="23" y="30" />
				</Anchors>
			</Frame>
		</Frames>
	</Frame>
</Ui>
