<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\Blizzard_SharedXML\UI.xsd">
	<Frame name="PRCCharacterSelect" parent="CharSelectServicesFlowFrame" inherits="CharacterServicesBlockTemplate" hidden="true" frameStrata="DIALOG">
		<Size x="300" y="250"/>
		<Frames>
			<Frame parentKey="ControlsFrame" setAllPoints="true"/>
			<Frame parentKey="ResultsFrame">
				<Size x="260" y="250"/>
				<Anchors>
					<Anchor point="TOPLEFT" relativeKey="$parent.ResultsLabel" relativePoint="BOTTOMLEFT" y="-2"/>
				</Anchors>
				<Layers>
					<Layer level="ARTWORK">
						<FontString parentKey="CurrentRaceLabel" inherits="GameFontNormalMed3" justifyH="LEFT">
							<Size x="260" y="0"/>
							<Anchors>
								<Anchor point="TOPLEFT"/>
							</Anchors>
							<Color color="HIGHLIGHT_FONT_COLOR"/>
						</FontString>
						<FontString parentKey="CustomizeLabel" inherits="GameFontNormal" justifyH="LEFT" text="PRC_FLOW_CUSTOMIZE_CHARACTER_STEP">
							<Size x="260" y="0"/>
							<Anchors>
								<Anchor point="TOPLEFT" relativeKey="$parent.CurrentRaceLabel" relativePoint="BOTTOMLEFT" y="-38"/>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
			</Frame>
		</Frames>
	</Frame>
</Ui>