<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">

    <Script file="Blizzard_ClassTalentImportDialog.lua"/>

    <Button name="ClassTalentImportDialogButtonTemplate" inherits="UIPanelButtonTemplate, UIButtonTemplate" virtual="true">
        <Size x="130" y="22"/>
    </Button>

    <Frame name="ClassTalentImportDialog" parent="UIParent" hidden="true" frameStrata="DIALOG" mixin="ClassTalentImportDialogMixin" frameLevel="100">
        <Size x="320" y="300"/>
        <Anchors>
            <Anchor point="CENTER"/>
        </Anchors>
        <Layers>
            <Layer level="ARTWORK">
                <FontString parentKey="Title" inherits="GameFontHighlight" text="HUD_CLASS_TALENTS_IMPORT_DIALOG_TITLE">
                    <Size x="250"/>
                    <Anchors>
                        <Anchor point="TOP" y="-20"/>
                    </Anchors>
                </FontString>
                <FontString parentKey="EditBoxLabel" inherits="GameFontNormal" text="HUD_CLASS_TALENTS_IMPORT_DIALOG_EDIT_BOX_LABEL">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="22" y="-40"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Frame parentKey="Border" inherits="DialogBorderTemplate"/>
            <ScrollFrame parentKey="ImportBox" inherits="InputScrollFrameTemplate">
                <Size x="270" y="175"/>
                <Anchors>
                    <Anchor point="TOP" y="-60"/>
                </Anchors>
                <KeyValues>
                    <KeyValue key="maxLetters" value="1000" type="number"/>
                    <KeyValue key="instructions" value="HUD_CLASS_TALENTS_IMPORT_INSTRUCTIONS" type="global"/>
                    <KeyValue key="hideCharCount" value="true" type="boolean"/>
                </KeyValues>
            </ScrollFrame>
            <Button parentKey="AcceptButton" inherits="ClassTalentImportDialogButtonTemplate" text="HUD_CLASS_TALENTS_IMPORT_LOADOUT_ACCEPT_BUTTON">
                <KeyValues>
                    <KeyValue key="disabledTooltip" value="HUD_CLASS_TALENTS_IMPORT_ERROR_IMPORT_STRING_AND_NAME" type="global"/>
                </KeyValues>
                <Anchors>
                    <Anchor point="BOTTOMLEFT" x="25" y="25"/>
                </Anchors>
            </Button>
            <Button parentKey="CancelButton" inherits="ClassTalentImportDialogButtonTemplate" text="CANCEL">
                <Anchors>
                    <Anchor point="BOTTOMRIGHT" x="-25" y="25"/>
                </Anchors>
            </Button>
        </Frames>
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnHide method="OnHide"/>
        </Scripts>
    </Frame>
</Ui>