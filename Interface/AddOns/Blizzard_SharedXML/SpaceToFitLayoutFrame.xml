<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
UI.xsd">
	<Frame name="SpaceToFitVerticalLayoutFrame" mixin="SpaceToFitVerticalLayoutMixin" inherits="VerticalLayoutFrame" virtual="true">
		<KeyValues>
			<!-- 
			baseSpacing: spacing to use when child elements fit within the specified bounds.
			-->
			<KeyValue key="baseSpacing" value="0" type="number"/>
		</KeyValues>
	</Frame>

	<Frame name="SpaceToFitHorizontalLayoutFrame" mixin="SpaceToFitHorizontalLayoutMixin" inherits="HorizontalLayoutFrame" virtual="true">
		<KeyValues>
			<!-- 
			baseSpacing: spacing to use when child elements fit within the specified bounds.
			-->
			<KeyValue key="baseSpacing" value="0" type="number"/>
		</KeyValues>
	</Frame>
</Ui>
