			<xs:enumeration value="korean"/>
			<xs:enumeration value="simplifiedchinese"/>
			<xs:enumeration value="traditionalchinese"/>
			<xs:enumeration value="russian"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="WRAPMODE">
		<xs:restriction base="xs:NMTOKEN">
