		<xs:attribute name="x" type="xs:float" use="optional"/>
		<xs:attribute name="y" type="xs:float" use="optional"/>
	</xs:complexType>

	<xs:element name="AbsInset">
		<xs:complexType>
			<xs:attribute name="left" type="xs:float" use="optional"/>
