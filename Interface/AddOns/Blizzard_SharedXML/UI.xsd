	<xs:complexType name="LayoutFrameType">
		<xs:complexContent>
			<xs:extension base="LayoutFrameRefType">
				<xs:sequence minOccurs="0">
					<xs:element ref="LayoutField"/>
				</xs:sequence>
				<xs:attributeGroup ref="LayoutFrameAttributes"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:element name="LayoutField" abstract="true"/>
	<xs:element name="Size" type="Dimension" substitutionGroup="LayoutField"/>
	<xs:element name="Anchors" substitutionGroup="LayoutField">
		<xs:complexType>
			<xs:sequence minOccurs="0">
				<xs:element name="Anchor" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence minOccurs="0">
							<xs:element name="Offset" type="Dimension"/>
						</xs:sequence>
						<xs:attribute name="point" type="FRAMEPOINT" use="required"/>
						<xs:attribute name="relativeKey" type="xs:string"/>
						<xs:attribute name="relativeTo" type="xs:string"/>
						<xs:attribute name="relativePoint" type="FRAMEPOINT"/>
						<xs:attribute name="x" type="xs:float"/>
						<xs:attribute name="y" type="xs:float"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Scripts" type="ScriptsType" substitutionGroup="LayoutField"/>
	<xs:element name="KeyValues" type="KeyValuesType" substitutionGroup="LayoutField"/>
	<xs:element name="Animations" substitutionGroup="LayoutField">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AnimationGroup" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LayoutFrame" type="LayoutFrameType"/>

	<xs:complexType name="RectType">
		<xs:attribute name="ULx" type="xs:float"/>
		<xs:attribute name="ULy" type="xs:float"/>
		<xs:attribute name="LLx" type="xs:float"/>
		<xs:attribute name="LLy" type="xs:float"/>
		<xs:attribute name="URx" type="xs:float"/>
		<xs:attribute name="URy" type="xs:float"/>
		<xs:attribute name="LRx" type="xs:float"/>
		<xs:attribute name="LRy" type="xs:float"/>
	</xs:complexType>

