			<xs:attribute name="y" type="xs:float" use="optional"/>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="Dimension">
		<xs:choice minOccurs="0">
			<xs:element ref="AbsDimension"/>
			<xs:element ref="RelDimension"/>
