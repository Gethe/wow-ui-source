<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\UI.xsd">
	<Frame name="DropdownLoadSystemTemplate" mixin="DropdownLoadSystemMixin" virtual="true">
		<Size x="200" y="30"/>
		<Frames>
			<DropdownButton parentKey="Dropdown" inherits="WowStyle1DropdownTemplate">
				<KeyValues>
					<KeyValue key="menuPoint" type="string" value="BOTTOMLEFT"/>
					<KeyValue key="menuRelativePoint" type="string" value="TOPLEFT"/>
				</KeyValues>
				<Anchors>
					<Anchor point="LEFT"/>
				</Anchors>
			</DropdownButton>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>
</Ui>