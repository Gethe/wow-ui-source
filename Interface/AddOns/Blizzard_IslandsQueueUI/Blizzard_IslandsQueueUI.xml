				</Anchors>
				<Layers>
					<Layer level="BORDER">
						<Texture parentKey="Icon" atlas="honorsystem-prestige-laurel-bg-alliance">
							<Size x="29" y="29"/>
							<Anchors>
								<Anchor point="CENTER" x="1"/> 
							</Anchors>
						</Texture>
						<MaskTexture parentKey="CircleMask" file="Interface\CharacterFrame\TempPortraitAlphaMask" hWrapMode="CLAMPTOBLACKADDITIVE" vWrapMode="CLAMPTOBLACKADDITIVE">
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
							<MaskedTextures>
								<MaskedTexture childKey="Icon"/>
							</MaskedTextures>
						</MaskTexture>
					</Layer>
					<Layer level="OVERLAY">
						<Texture parentKey="CompletedCheck" atlas="islands-queue-progressbar-checkmark" useAtlasSize="true" hidden="true">
							<Anchors>
								<Anchor point="CENTER"/> 
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
				<Frames>
					<Frame parentKey="Tooltip" inherits="VerticalLayoutFrame, TooltipBackdropTemplate" frameStrata="TOOLTIP" clampedToScreen="true" hidden="true">
						<Anchors>
							<Anchor point="BOTTOMLEFT" relativeKey="$parent.Icon" relativePoint="TOPRIGHT" x="0" y="0"/>
						</Anchors>
						<KeyValues>
							<KeyValue key="fixedWidth" value="100" type="number"/>
							<KeyValue key="expand" value="true" type="boolean"/>
							<KeyValue key="topPadding" value="10" type="number"/>
							<KeyValue key="bottomPadding" value="12" type="number"/>
							<KeyValue key="leftPadding" value="5" type="number"/>
							<KeyValue key="rightPadding" value="5" type="number"/>
							<KeyValue key="spacing" value="8" type="number"/>
						</KeyValues>
						<Anchors>
							<Anchor point="BOTTOMRIGHT"/>
						</Anchors>
						<Frames>
							<Frame parentKey="ItemTooltip" inherits="InternalEmbeddedItemTooltipTemplate">
								<KeyValues>
									<KeyValue key="leftPadding" value="5" type="number"/>
									<KeyValue key="layoutIndex" value="2" type="number"/>
								</KeyValues>
								<Size x="100" y="100"/>
								<Scripts>
									<OnSizeChanged inherit="prepend">
										self:GetParent():Layout();
									</OnSizeChanged>
								</Scripts>
							</Frame>
						</Frames>
					</Frame>
				</Frames>
				<Scripts>
					<OnEnter>
						if (self.Completed) then 
							GameTooltip:SetOwner(self.Icon, "ANCHOR_TOPLEFT", 25, 0);
							GameTooltip:SetText(GOAL_COMPLETED, GREEN_FONT_COLOR:GetRGB());
							GameTooltip:Show();
						else 
							QuestUtils_AddQuestCurrencyRewardsToTooltip(self:GetParent().questID, self.Tooltip, self.Tooltip.ItemTooltip);
							self.Tooltip:Show(); 
						end
					</OnEnter>
					<OnLeave>
						self.Tooltip:Hide(); 
						GameTooltip:Hide();
					</OnLeave>
				</Scripts>
			</Button>
		</Frames>
		<Scripts>
			<OnEvent method="OnEvent"/>
			<OnShow method="OnShow"/>
			<OnHide method="OnHide"/>
			<OnEnter>
				self.OverlayFrame.Text:Show(); 
			</OnEnter>
			<OnLeave>
				self.OverlayFrame.Text:Hide(); 
			</OnLeave>
		</Scripts>
