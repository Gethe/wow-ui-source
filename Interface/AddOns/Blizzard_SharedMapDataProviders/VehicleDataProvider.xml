<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Script file="VehicleDataProvider.lua"/>

	<Frame name="VehiclePinTemplate" hidden="true" enableMouse="true" mixin="VehiclePinMixin" virtual="true">
		<Size x="45" y="45"/>
		<Anchors>
			<Anchor point="CENTER"/>
		</Anchors>
		<Layers>
			<Layer level="ARTWORK">
				<Texture parentKey="Texture" setAllPoints="true"/>
			</Layer>
		</Layers>
		<Scripts>
			<OnUpdate method="OnUpdate"/>
		</Scripts>
	</Frame>
</Ui>