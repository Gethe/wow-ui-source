<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Script file="DeathMapDataProvider.lua"/>

	<Frame name="CorpsePinTemplate" mixin="CorpsePinMixin" enableMouse="true" hidden="true" virtual="true">
		<Size x="24" y="24"/>
		<Anchors>
			<Anchor point="CENTER"/>
		</Anchors>
		<Layers>
			<Layer level="ARTWORK">
				<Texture file="Interface\Minimap\POIIcons" setAllPoints="true">
					<TexCoords left="0.875" right="1.0" top="0.0" bottom="0.125"/>
				</Texture>
			</Layer>
		</Layers>
	</Frame>

	<Frame name="DeathReleasePinTemplate" mixin="DeathReleasePinMixin" inherits="CorpsePinTemplate" virtual="true">
		<Size x="24" y="24"/>
	</Frame>
</Ui>