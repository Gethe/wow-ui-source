<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Script file="PropertyFontString.lua"/>

	<Frame name="PropertyFontStringTemplate" mixin="PropertyBindingMixin, PropertyFontStringMixin" virtual="true">
		<Layers>
			<Layer level="ARTWORK">
				<FontString parentKey="Text" inherits="GameFontHighlightSmall" justifyH="RIGHT" justifyV="CENTER" setAllPoints="true"/>
			</Layer>
		</Layers>
		<Scripts>
			<OnEvent method="OnEvent"/>
		</Scripts>
	</Frame>
</Ui>