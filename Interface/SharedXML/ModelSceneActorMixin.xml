<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI_shared.xsd">
	<Include file="ModelSceneActorMixinScripts.xml"/>

	<ScopedModifier scriptsUseGivenEnv="true">
		<Actor name="ModelSceneActorTemplate" mixin="ModelSceneActorMixin" virtual="true">
			<Scripts>
				<OnModelCleared method="OnModelCleared" />
				<OnModelLoaded method="OnModelLoaded" />
				<OnUpdate method="OnUpdate" />
			</Scripts>
		</Actor>
	</ScopedModifier>
</Ui>
