<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI_shared.xsd">
	<Script file="EventButton.lua"/>

	<Button name="EventButton" mixin="EventButtonMixin" enableMouse="true" intrinsic="true">
		<Scripts>
			<OnLoad method="OnLoad_Intrinsic"/>
			<OnMouseUp method="OnMouseUp_Intrinsic" intrinsicOrder="postcall"/>
			<OnMouseDown method="OnMouseDown_Intrinsic" intrinsicOrder="postcall"/>
			<OnClick method="OnClick_Intrinsic" intrinsicOrder="postcall"/>
			<OnEnter method="OnEnter_Intrinsic" intrinsicOrder="postcall"/>
			<OnLeave method="OnLeave_Intrinsic" intrinsicOrder="postcall"/>
			<OnSizeChanged method="OnSizeChanged_Intrinsic" intrinsicOrder="postcall"/>
		</Scripts>
	</Button>
</Ui>