					<Size>
						<AbsDimension x="128" y="16"/>
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset x="-2" y="2"/>
						</Anchor>
						<Anchor point="TOPRIGHT">
							<Offset x="2" y="-2"/>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
