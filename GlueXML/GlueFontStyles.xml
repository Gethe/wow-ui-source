<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Script>
		NORMAL_FONT_COLOR = {r=1.0, g=0.82, b=0};
		HIGHLIGHT_FONT_COLOR = {r=1.0, g=1.0, b=1.0};
		GRAY_FONT_COLOR = {r=0.5, g=0.5, b=0.5};
		GREEN_FONT_COLOR = {r=0.1, g=1.0, b=0.1};
		RED_FONT_COLOR = {r=1.0, g=0.1, b=0.1};
		BLUE_FONT_COLOR = {r=0, g=0.749, b=0.953};
	</Script>

	<!-- This GlueFonts.xml is loaded before GlueFontStyles.xml Set up Font objects to be localized in GlueFonts.xml not GlueFontStyles.xml -->

	<Font name="GlueFontNormal" inherits="SystemFont_Outline_Med2" virtual="true">
		<Shadow>
			<Offset>
				<AbsDimension x="1" y="-1"/>
			</Offset>
			<Color r="0" g="0" b="0"/>
		</Shadow>
		<Color r="1.0" g="0.78" b="0"/>
	</Font>
	<Font name="GlueFontNormalLeft" inherits="GlueFontNormal" justifyH="LEFT" virtual="true"/>
	<Font name="GlueFontHighlight" inherits="GlueFontNormal" virtual="true">
		<Color r="1.0" g="1.0" b="1.0"/>
	</Font>
	<Font name="GlueFontHighlightLeft" inherits="GlueFontHighlight" justifyH="LEFT" virtual="true"/>
	<Font name="RealmDownNormal" inherits="GlueFontNormalLeft" virtual="true">
		<Color r="0.5" g="0.5" b="0.5"/>
	</Font>
	<Font name="RealmDownHighlight" inherits="GlueFontHighlightLeft" virtual="true">
		<Color r="0.8" g="0.8" b="0.8"/>
	</Font>
	<Font name="RealmInvalidNormal" inherits="GlueFontNormalLeft" virtual="true">
		<Color r="1.0" g="0.1" b="0.1"/>
	</Font>
	<Font name="RealmCharactersNormal" inherits="GlueFontNormalLeft" virtual="true">
		<Color r="0.1" g="1.0" b="0.1"/>
	</Font>
	<Font name="RealmNoCharactersNormal" inherits="GlueFontNormalLeft" virtual="true">
