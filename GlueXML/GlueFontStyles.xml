		<Color r="1.0" g="0.78" b="0"/>
	</Font>
	<Font name="GlueFontNormalLargeLeft" inherits="GlueFontNormalLarge" justifyH="LEFT" virtual="true"/>
	<Font name="GlueFontNormalLargeLeftTop" inherits="GlueFontNormalLargeLeft" justifyV="TOP" virtual="true"/>
