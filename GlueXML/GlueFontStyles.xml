		<Color r="1.0" g="0.5" b="0.5"/>
	</Font>
	<Font name="GlueFontDisable" inherits="GlueFontNormal" virtual="true">
		<Color r="0.5" g="0.5" b="0.5"/>
	</Font>
	<Font name="GlueFontDisableLeft" inherits="GlueFontDisable" justifyH="LEFT" virtual="true"/>
	<Font name="GlueFontGreen" inherits="GlueFontNormal" virtual="true">
		<Color r="0.1" g="1.0" b="0.1"/>
	</Font>
	<Font name="GlueFontRed" inherits="GlueFontNormal" virtual="true">
		<Color r="1.0" g="0.1" b="0.1"/>
	</Font>
	<Font name="GlueFontLightYellow" inherits="GlueFontNormal" virtual="true">
		<Color r="1.0" g="0.96" b="0.62"/>
