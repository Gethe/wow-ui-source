		<Scripts>
			<OnClick>
				CharacterClass_OnClick(self, self.classID);	
				GlueTooltip:Hide();
			</OnClick>
			<OnEnter>
				if ( CharacterCreate.selectedClass == self:GetID() ) then
					return;
				end
				GlueTooltip_SetOwner(nil,nil, -3, -5);
				GlueTooltip:SetText(self.tooltip, nil, 1.0, 1.0, 1.0);
			</OnEnter>
			<OnLeave>
				GlueTooltip:Hide();
			</OnLeave>
		</Scripts>
