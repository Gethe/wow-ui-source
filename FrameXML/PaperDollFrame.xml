                PaperDollItemSlotButton_OnEvent(event);
            </OnEvent>
            <OnClick>
                PaperDollItemSlotButton_OnClick(arg1);
            </OnClick>
            <OnShow>
            </OnShow>
            <OnDragStart>
                PaperDollItemSlotButton_OnClick("LeftButton", 1);
            </OnDragStart>
