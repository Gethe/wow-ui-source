					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="0" y="-1"/>
							</Offset>
						</Anchor>
					</Anchors>
					<TexCoords left="0" right="0.544921875" top="0.849609375" bottom="0.87109375"/>
