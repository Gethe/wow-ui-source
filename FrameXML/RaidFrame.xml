						else
							StaticPopup_Show("ADD_RAIDMEMBER");
						end
					</OnClick>
				</Scripts>
				<NormalText inherits="GameFontNormalSmall"/>
				<HighlightText inherits="GameFontHighlightSmall"/>
				<DisabledText inherits="GameFontDisableSmall"/>
			</Button>
