		<Scripts>
			<OnLoad>
				this:SetFrameLevel(this:GetFrameLevel() + 2);
				this:RegisterForClicks("LeftButtonUp", "RightButtonUp");
			</OnLoad>
			<OnMouseDown>
				RaidGroupButton_OnMouseDown(arg1);
			</OnMouseDown>
			<OnMouseUp>
