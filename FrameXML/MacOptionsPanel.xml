							<Offset>
								<AbsDimension x="0" y="-9"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="MovieRecordingOptionsFrameText4" inherits="GameFontWhite">
					<Anchors>
						<Anchor point="LEFT" relativeTo="MovieRecordingOptionsFrameText3" relativePoint="RIGHT">
