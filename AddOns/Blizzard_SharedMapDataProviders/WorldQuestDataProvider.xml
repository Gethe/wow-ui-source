<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Script file="WorldQuestDataProvider.lua"/>

	<Frame name="WorldQuestPinTemplate" hidden="true" flattenRenderLayers="true" enableMouseMotion="true" mixin="WorldQuestPinMixin" virtual="true">
		<Size x="50" y="50"/>

		<Layers>
			<Layer level="BACKGROUND" textureSubLevel="-2">
				<Texture parentKey="Underlay">
					<Size x="72" y="72" />
					<Anchors>
						<Anchor point="CENTER" x="0" y="-3" />
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="BACKGROUND" textureSubLevel="-1">
				<Texture parentKey="Background">
					<Anchors>
						<Anchor point="CENTER" />
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<Texture parentKey="Texture">
					<Anchors>
						<Anchor point="CENTER" />
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="HIGHLIGHT">
				<Texture parentKey="Highlight" alphaMode="ADD">
					<Anchors>
						<Anchor point="CENTER" />
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Frame parentKey="TimeLowFrame">
				<Size x="44" y="44" />
				<Anchors>
					<Anchor point="CENTER" x="-17" y="-17" />
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<Texture parentKey="Icon" atlas="worldquest-icon-clock" />
					</Layer>
				</Layers>
			</Frame>
		</Frames>
	</Frame>
</Ui>
