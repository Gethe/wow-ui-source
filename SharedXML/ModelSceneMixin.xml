<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Include file="ModelSceneMixinScripts.xml"/>

	<ScopedModifier scriptsUseGivenEnv="true">
		<ModelScene name="ModelSceneMixinTemplate" mixin="ModelSceneMixin" virtual="true">
			<Scripts>
				<OnLoad method="OnLoad" />
				<OnUpdate method="OnUpdate" />
				<OnMouseUp method="OnMouseUp"/>
				<OnMouseDown method="OnMouseDown"/>
				<OnMouseWheel method="OnMouseWheel" />
			</Scripts>
		</ModelScene>
		<ModelScene name="ReversedLightingModelSceneMixinTemplate" inherits="ModelSceneMixinTemplate" virtual="true">
			<KeyValues>
				<KeyValue key="reversedLighting" value="true" type="boolean"/>
			</KeyValues>
		</ModelScene>

	</ScopedModifier>
</Ui>
